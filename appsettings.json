{
  "ReverseProxy": {
    "Routes": {
      "authRoute": {
        "ClusterId": "authCluster",
        "Match": { "Path": "/auth/{**catch-all}" }
      }
    },
    "Clusters": {
      "authCluster": {
        "Destinations": {
          "a1": { "Address": "http://auth1:5000" },
          "a2": { "Address": "http://auth2:5000" }
        }
      }
    }
  }
}
