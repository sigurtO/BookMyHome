{
  "ReverseProxy": {
    "Routes": {
      "authRoute": {
        "ClusterId": "authCluster",
        "Match": { "Path": "/auth/{**catch-all}" }
      }
    },
    "Clusters": {
      "authCluster": {
        "Destinations": {
          "auth": { "Address": "http://authenticationservice:5000" }
        }
      }
    }
  }
}
